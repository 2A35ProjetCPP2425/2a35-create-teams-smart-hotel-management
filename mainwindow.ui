        <string notr="true">/* Main Window background */
QMainWindow {
    background-color: #f8f8f8;
}

/* Group Box styling for &quot;Information Client&quot; */
QGroupBox {
    background-color: #ffffff;
    border: 2px solid #00c4cc;
    border-radius: 10px;
    font-size: 16px;
    font-weight: bold;
    color: #333333;
    padding: 10px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    background-color: #00e1e8;
    padding: 5px;
    border-radius: 5px;
    color: #ffffff;
    font-weight: bold;
}

/* Input fields (QLineEdit) */
QLineEdit {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;
}

QLineEdit:focus {
    border-color: #00c4cc;
}

/* Dropdowns (QComboBox) */
QComboBox {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    padding: 5px;
    font-size: 14px;
    color: #333333;
    border-radius: 5px;
}

QComboBox::drop-down {
    border: none;
}

/* Buttons */
QPushButton {
    background-color: #3498db;
    color: #ffffff;
    font-size: 14px;
    padding: 8px;
    border-radius: 8px;
    border: none;
}

QPushButton:hover {
    background-color: #2980b9;
}

QPushButton:pressed {
    background-color: #1f6391;
}

QPushButton#addClientButton {
    background-color: #28a745; /* Specific green color for 'add' button */
}

QPushButton#deleteClientButton {
    background-color: #e74c3c; /* Specific red color for 'delete' button */
}

/* Labels */
QLabel {
    font-size: 14px;
    font-weight: bold;
    color: #333333;
}

/* Table Widget */
QTableWidget {
    background-color: #ffffff;
    alternate-background-color: #f2f2f2;
    gridline-color: #dcdcdc;
    font-size: 14px;
}

QHeaderView::section {
    background-color: #3498db;
    color: #ffffff;
    font-weight: bold;
    font-size: 14px;
    padding: 5px;
    border: none;
}

QTableWidget QTableCornerButton::section {
    background-color: #3498db;
}

/* Scrollbars */
QScrollBar:vertical, QScrollBar:horizontal {
    background-color: #e0e0e0;
    width: 10px;
}

QScrollBar::handle:vertical, QScrollBar::handle:horizontal {
    background-color: #3498db;
    min-height: 20px;
    border-radius: 5px;
}

QScrollBar::handle:vertical:hover, QScrollBar::handle:horizontal:hover {
    background-color: #2980b9;
}
</string>
