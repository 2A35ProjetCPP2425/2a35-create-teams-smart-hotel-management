       <property name="styleSheet">
        <string notr="true">/* Style pour QDateEdit */
QDateEdit {
    background-color: #f8f8f0; /* Blanc cassé */
    color: #000000; /* Texte noir */
    border: 2px solid #007bff; /* Bordure bleue */
    border-radius: 5px; /* Coins arrondis */
    padding: 6px; /* Espacement interne */
    font-size: 14px; /* Taille de la police */
    min-width: 120px; /* Largeur minimale pour le champ de date */
}

/* Style au survol de QDateEdit */
QDateEdit:hover {
    border-color: #0056b3; /* Bordure bleu foncé au survol */
}

/* Style pour QDateEdit lorsqu'il est en focus */
QDateEdit:focus {
    border-color: #00bfff; /* Bordure bleu ciel au focus */
    box-shadow: 0 0 8px #00bfff; /* Ombre bleue au focus */
}

/* Style pour la flèche de descente du QDateEdit */
QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid #007bff; /* Séparation entre le champ et le bouton de descente */
    background-color: #007bff; /* Fond bleu pour le bouton */
    border-top-right-radius: 5px; /* Coins arrondis en haut à droite */
    border-bottom-right-radius: 5px; /* Coins arrondis en bas à droite */
}

/* Style pour l'icône de la flèche de descente */
QDateEdit::down-arrow {
    image: url(:/images/down-arrow.png); /* Remplacer par l'icône personnalisée de flèche vers le bas */
    width: 12px; /* Largeur de l'icône */
    height: 12px; /* Hauteur de l'icône */
}

/* Optionnel: Style au survol du bouton de descente */
QDateEdit::drop-down:hover {
    background-color: #0056b3; /* Changement de couleur au survol */
}

/* Optionnel: Style des boutons fléchés dans l'affichage du calendrier */
QCalendarWidget QToolButton {
    color: #ffffff; /* Couleur du texte des boutons du calendrier */
    background-color: #007bff; /* Fond bleu pour les boutons du calendrier */
    border: none; /* Pas de bordure pour les boutons */
    border-radius: 4px; /* Coins légèrement arrondis */
    padding: 5px;
}

/* Style pour les boutons fléchés (mois précédent/suivant) */
QCalendarWidget QToolButton::hover {
    background-color: #0056b3; /* Fond bleu foncé au survol */
}

/* Style des numéros dans le calendrier */
QCalendarWidget QAbstractItemView {
    background-color: #ffffff; /* Fond blanc pour les jours */
    selection-background-color: #007bff; /* Fond bleu pour le jour sélectionné */
    selection-color: white; /* Texte blanc pour le jour sélectionné */
    color: #000000; /* Texte noir pour les jours non sélectionnés */
}
